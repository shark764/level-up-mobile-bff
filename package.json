{
  "name": "mobile-bff",
  "version": "1.0.0",
  "description": "Mobile bff APIS LUL",
  "main": "server/index.js",
  "engines": {
    "node": "14.16.1"
  },
  "scripts": {
    "build": "babel server --out-dir dist",
    "dev": "nodemon --exec npx babel-node server/index.js",
    "format": "prettier-eslint --write $PWD/\"server/**/*.+(js|ts|json|html)\"",
    "lint": "eslint ./server",
    "lint:fix": "eslint ./server --fix",
    "postinstall": "rm -rf dist && npm run build",
    "pretest": "eslint --ignore-path .gitignore .",
    "prettify": "npm run format && npm run lint:fix",
    "release": "release-it patch --no-npm.publish",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|ts|json|css|html)": "npm run prettify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/focus-ventures/level-up-mobile-bff.git"
  },
  "keywords": [
    "api",
    "express",
    "mobile",
    "mongodb",
    "nodejs",
    "rest"
  ],
  "author": "Fernando Rivas",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/focus-ventures/level-up-mobile-bff/issues"
  },
  "homepage": "https://gitlab.com/focus-ventures/level-up-mobile-bff#readme",
  "dependencies": {
    "aws-sdk": "^2.886.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "ioredis": "^4.23.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.19",
    "multer": "^1.4.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.24.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "2.4.1",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier-eslint-cli": "^5.0.1"
  }
}
